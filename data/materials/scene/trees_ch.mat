//  CH trees
//-----------------------------------------------------------------------------------------------------------------

//  trunk
material base-tree
{
    parent base
    diffuseMap trunk2m.png
    //diffuse 0.8 0.8 0.8

    tree_wind true
    terrain_light_map true
    //diffuse_address_mode mirror
}

//  leaves
material base-twig
{
    parent base
    alpha_rejection greater 128
    diffuseMap twig21_0yo.png
    ambient 1.3 1.3 1.3
    diffuse 0.9 0.9 0.9
    specular 0.2 0.2 0.2 32

    tree_wind true
    terrain_light_map true
    cull_hardware none
    transparent true
    twoside_diffuse true
    receives_shadows true
    shadowBias 0.0001
}


