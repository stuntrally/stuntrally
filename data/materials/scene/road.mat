///  next check marker
material checkpoint
{
	parent base
	diffuseMap checkpoint.png
	ambient 0.1 0.1 0.1
	diffuse 0.1 0.1 0.1
	specular 0.2 0.4 0.2  32 //x4
	emissive 0.1 11.0 0.1

	depth_write off
	scene_blend alpha_blend
	receives_shadows false

	//specular_alpha true
	cull_hardware none
	twoside_diffuse true
}


material road_terrain
{
	parent base
	depth_write off
	scene_blend alpha_blend
	alphaMap roadAlpha.png
	//alphaMap noise.jpg
	depth_bias 3
	terrain_light_map true
	road_blend true
}

